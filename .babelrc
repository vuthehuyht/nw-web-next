{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "cwd": "babelrc",
          "components": "./components",
          "styles": "./styles"
        }
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
  ]
}
  